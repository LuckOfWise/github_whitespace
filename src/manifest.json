{
	"name": "Github Whitespace",
	"version": "0.1",
	"manifest_version": 2,
	
	"description": "Githubのアドレスで差分を見るとき空白を無視するオプション『?w=1』をつけてリダイレクトするエクステンションです。",
	
	"permissions": [
		"tabs", "https://*/*"
	],
	
	"background": {
		"scripts": ["background.js"]
	},
	
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Github Whitespace"
	}
}